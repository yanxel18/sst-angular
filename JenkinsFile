pipeline{
  agent any
    stages {
      stage('Install Dependency') {
        steps{
          bat 'npm install --force'
        }
      }
    stage('SonarQube analysis') {
      steps {
        bat 'npm run sonar'
      }
    }
    stage('Quality Gate') {
      steps {
        waitForQualityGate abortPipeline: true
      }
    }
  }
}
